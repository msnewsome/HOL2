<p id="notice"><%= notice %></p>

<h1>App Times</h1>

<table>
  <thead>
    <tr>
      <th>Appdate</th>
      <th>Booked?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_times.each do |app_time| %>
      <tr>
        <td><%= app_time.appDate %></td>
        <td><%= app_time.booked %></td>
        <td><%= link_to 'Show', app_time %></td>
        <td><%= link_to 'Edit', edit_app_time_path(app_time) %></td>
        <td><%= link_to 'Destroy', app_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Time', new_app_time_path %>
